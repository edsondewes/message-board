{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/recommended"],
  "rules": {
    "no-console": 0,
    "space-before-function-paren": [
      "warn",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
